    }
});
$( "#link_prop_submit" ).click(function() {
  var link = $('input[name=link_prop]').val();
  var lang = $('select[name=version] option:selected').val();
  var movieId = '18697';

   $.ajax({
        url: '/movie/propMovie',
        type: 'post',
        data: {link: link, lang: lang, movieId: movieId},
        success: function(response) {
        	$('#prop_resp').html(response);
            $( "#prop_resp" ).fadeIn( "slow" );
        }
    });
});
</script>
